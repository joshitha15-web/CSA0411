#include <stdio.h>
#define MAX 100

int main() {
    int file[MAX], n, i;
    printf("Enter number of records: ");
    scanf("%d", &n);

    for (i = 0; i < n; i++) {
        file[i] = i + 1;
    }

    printf("Accessing records sequentially:\n");
    for (i = 0; i < n; i++) {
        printf("Record %d\n", file[i]);
    }
    return 0;
}
