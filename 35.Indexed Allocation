#include <stdio.h>
#define MAX 100

int main() {
    int indexBlock[MAX], n;
    printf("Enter number of blocks: ");
    scanf("%d", &n);

    for (int i = 0; i < n; i++) {
        indexBlock[i] = i + 100; // Simulated block address
    }

    printf("Indexed Allocation:\n");
    for (int i = 0; i < n; i++) {
        printf("Block %d -> Address %d\n", i, indexBlock[i]);
    }
    return 0;
}
